{
  "name": "@compass-labs/sdk",
  "version": "1.0.175",
  "description": "Package structured tools for AI agents using compass API.",
  "keywords": [
    "compass-api",
    "compass-sdk",
    "web3-agents",
    "ai-agents",
    "agent-tools"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup --format cjs,esm --dts",
    "dev": "tsup --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "biome check src/",
    "lint:fix": "biome check --apply src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "generate:typescript_client": "tsc && node ./out/generate_client.js"
  },
  "author": "royalnine",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "fs-extra": "^11.3.0",
    "langchain": "^0.3.15",
    "openapi-zod-client": "^1.18.3",
    "openapi3-ts": "3.0.2",
    "prettier": "^3.5.0",
    "@apidevtools/swagger-parser": "^10.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^1.2.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^1.2.2"
  },
  "files": [
    "dist"
  ]
}
